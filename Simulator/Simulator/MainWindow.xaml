<Window x:Class="LabTomasulo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0" VerticalAlignment="Top" Height="26">
        <Menu x:Name="File">
            <MenuItem Header="File" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" Padding="6,0" Height="26" >
                <MenuItem Header="Open" Click="OpenBtn_Click"/>
                <MenuItem Header="Exit" Click="ExitBtn_Click"/>
            </MenuItem>
            <MenuItem Header="Help"/>
        </Menu>
        <TextBox x:Name="myTextBox" HorizontalAlignment="Left" Height="23" Margin="189,158,0,-155" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
